<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BunkDetails">

    <TextView
        android:id="@+id/bunkName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/contrail_one"
        android:gravity="center"
        android:hint="@string/bunkname"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textIsSelectable="false"
        android:textSize="23sp"
        app:layout_constraintBottom_toTopOf="@+id/circle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/circle"
        android:layout_width="288dp"
        android:layout_height="219dp"
        android:background="@drawable/circle"
        app:layout_constraintBottom_toTopOf="@+id/img00"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.791" />

    <TextView
        android:id="@+id/qlt00"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="QUALITY"
        app:layout_constraintBottom_toTopOf="@+id/quality"
        app:layout_constraintEnd_toEndOf="@+id/quality"
        app:layout_constraintHorizontal_bias="0.461"
        app:layout_constraintStart_toStartOf="@+id/quality"
        app:layout_constraintTop_toTopOf="@+id/circle"
        app:layout_constraintVertical_bias="0.676" />

    <TextView
        android:id="@+id/quality"
        android:layout_width="134dp"
        android:layout_height="50dp"
        android:fontFamily="@font/contrail_one"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/circle"
        app:layout_constraintEnd_toEndOf="@+id/circle"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="@+id/circle"
        app:layout_constraintTop_toBottomOf="@+id/bunkName" />

    <TextView
        android:id="@+id/pertentage"
        android:layout_width="25dp"
        android:layout_height="29dp"
        android:text="%"
        app:layout_constraintBottom_toBottomOf="@+id/circle"
        app:layout_constraintEnd_toEndOf="@+id/circle"
        app:layout_constraintHorizontal_bias="0.077"
        app:layout_constraintStart_toEndOf="@+id/quality"
        app:layout_constraintTop_toBottomOf="@+id/qlt00"
        app:layout_constraintVertical_bias="0.318" />

    <ImageView
        android:id="@+id/img00"
        android:layout_width="353dp"
        android:layout_height="113dp"
        android:background="@drawable/rectangle5"
        app:layout_constraintBottom_toTopOf="@+id/txt080"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/qnt00"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="QUANTITY"
        app:layout_constraintBottom_toTopOf="@+id/quantity"
        app:layout_constraintEnd_toEndOf="@+id/quantity"
        app:layout_constraintStart_toStartOf="@+id/quantity"
        app:layout_constraintTop_toTopOf="@+id/img00" />

    <TextView
        android:id="@+id/cst00"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="COST"
        app:layout_constraintBottom_toTopOf="@+id/petcost"
        app:layout_constraintEnd_toEndOf="@+id/petcost"
        app:layout_constraintStart_toStartOf="@+id/petcost"
        app:layout_constraintTop_toTopOf="@+id/img00" />

    <TextView
        android:id="@+id/petcost"
        android:layout_width="134dp"
        android:layout_height="50dp"
        android:fontFamily="@font/contrail_one"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/img00"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.817"
        app:layout_constraintStart_toStartOf="@+id/img00"
        app:layout_constraintTop_toTopOf="@+id/img00"
        app:layout_constraintVertical_bias="0.682" />

    <ImageView
        android:id="@+id/txt080"
        android:layout_width="375dp"
        android:layout_height="121dp"
        android:background="@drawable/rectangle6"
        app:layout_constraintBottom_toTopOf="@+id/cancell"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/temperature"
        android:layout_width="98dp"
        android:layout_height="50dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/txt080"
        app:layout_constraintEnd_toEndOf="@+id/txt080"
        app:layout_constraintHorizontal_bias="0.772"
        app:layout_constraintStart_toStartOf="@+id/txt080"
        app:layout_constraintTop_toBottomOf="@+id/img00"
        app:layout_constraintVertical_bias="0.513" />

    <TextView
        android:id="@+id/density"
        android:layout_width="113dp"
        android:layout_height="48dp"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/txt080"
        app:layout_constraintEnd_toStartOf="@+id/temperature"
        app:layout_constraintStart_toStartOf="@+id/txt080"
        app:layout_constraintTop_toTopOf="@+id/txt080" />

    <TextView
        android:id="@+id/quantity"
        android:layout_width="134dp"
        android:layout_height="50dp"
        android:fontFamily="@font/contrail_one"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/img00"
        app:layout_constraintEnd_toStartOf="@+id/petcost"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toStartOf="@+id/img00"
        app:layout_constraintTop_toTopOf="@+id/img00"
        app:layout_constraintVertical_bias="0.682" />

    <TextView
        android:id="@+id/den"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DENSITY"
        app:layout_constraintBottom_toTopOf="@+id/density"
        app:layout_constraintEnd_toEndOf="@+id/density"
        app:layout_constraintStart_toStartOf="@+id/density"
        app:layout_constraintTop_toBottomOf="@+id/img00" />

    <TextView
        android:id="@+id/temp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TEMPERATURE"
        app:layout_constraintBottom_toTopOf="@+id/temperature"
        app:layout_constraintEnd_toEndOf="@+id/temperature"
        app:layout_constraintStart_toStartOf="@+id/temperature"
        app:layout_constraintTop_toBottomOf="@+id/img00" />

    <ImageView
        android:id="@+id/img22"
        android:layout_width="9dp"
        android:layout_height="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/txt080"
        app:layout_constraintEnd_toStartOf="@+id/temp"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/temperature"
        app:layout_constraintTop_toTopOf="@+id/txt080"
        app:layout_constraintVertical_bias="0.132"
        app:srcCompat="@drawable/ellipse4" />

    <ImageView
        android:id="@+id/img33"
        android:layout_width="9dp"
        android:layout_height="8dp"
        app:layout_constraintBottom_toTopOf="@+id/density"
        app:layout_constraintEnd_toStartOf="@+id/den"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/density"
        app:layout_constraintTop_toBottomOf="@+id/img00"
        app:layout_constraintVertical_bias="0.39"
        app:srcCompat="@drawable/ellipse5" />

    <Button
        android:id="@+id/cancell"
        android:layout_width="330dp"
        android:layout_height="72dp"
        android:background="@drawable/rectangle1"
        android:gravity="center"
        android:text="BACK"
        android:textColor="#FFFF"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.567"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.969" />


</androidx.constraintlayout.widget.ConstraintLayout>